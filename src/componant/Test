import React, { Component } from 'react'

class Test extends Component{
    constructor(props){
        super(props)            
        this.count={
           count:0
         }
    }
    incriment(){
        this.state.count=this.state.count +1;
        console.log(this.state.count);
    }
    render()
    {
       
        return(
            <div>count -{this.state.count}
            <button onClick={ ()=> this.incriment()}></button>
            </div>
            
        )           
        
    }
}
export default Test
